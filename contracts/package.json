{
  "name": "contracts",
  "version": "1.0.0",
  "repository": "https://SeiyaKobayashi@github.com/SeiyaKobayashi/zkml-optimization.git",
  "author": "SeiyaKobayashi <seiya.kobayashi.work@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compile": "run-s compile-sol compile-ts",
    "compile-sol": "hardhat compile",
    "compile-ts": "tsc --pretty --noEmit",
    "lint": "run-s lint-sol lint-ts",
    "lint-sol": "solhint -c .solhint.json --ignore-path .solhintignore --fix src/*.sol src/**/*.sol",
    "lint-ts": "eslint --fix --ext .ts,.test.ts .",
    "prettify": "run-s prettify-sol prettify-ts",
    "prettify-sol": "prettier -w src/*.sol src/**/*.sol",
    "prettify-ts": "prettier -w **/*.ts",
    "test": "REPORT_GAS=true hardhat test --typecheck",
    "coverage": "hardhat coverage",
    "run-hardhat": "hardhat node",
    "deploy-contracts": "sh -c 'hardhat run --network ${0} scripts/deploy-contracts.ts'"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.8.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "1.0.5",
    "@nomicfoundation/hardhat-toolbox": "2.0.1",
    "@nomiclabs/hardhat-ethers": "2.2.2",
    "@nomiclabs/hardhat-etherscan": "3.1.5",
    "@openzeppelin/merkle-tree": "1.0.2",
    "@typechain/ethers-v5": "10.2.0",
    "@typechain/hardhat": "6.1.5",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "chai": "4.3.7",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethers": "5.7.2",
    "hardhat": "2.12.6",
    "hardhat-gas-reporter": "1.0.9",
    "prettier": "2.8.2",
    "prettier-plugin-solidity": "1.1.1",
    "solc": "0.8.17",
    "solhint": "3.3.7",
    "solidity-coverage": "0.8.2",
    "soltypes": "2.0.0",
    "ts-node": "10.9.1",
    "typechain": "8.1.1",
    "typescript": "4.9.4",
    "yarn-run-all": "3.1.1"
  }
}
